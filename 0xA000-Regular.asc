# ttfautohint-options: --no-info
fontfamily 0xA000
fontname 0xA000-Regular
variant regular
scale 200
y_shift -1
inline_components 1
spacing 0xA000.spacing
overlap_solid 0

include components-regular.asc
include palette-regular.asc

!! # marker for start of glyphs

include glyphs-ascii.asc
include glyphs-latin.asc
include glyphs-elaborated.asc
#include glyphs-smallcaps.asc
#include glyphs-debug.asc
